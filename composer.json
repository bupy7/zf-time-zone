{
  "name": "bupy7/zf-time-zone",
  "description": "A time zone module for Laminas and Mezzio.",
  "version": "2.0.0",
  "license": "BSD-3-Clause",
  "keywords": [
    "zend",
    "time",
    "zf",
    "zf3",
    "zone",
    "zend expressive",
    "expressive",
    "laminas",
    "mezzio"
  ],
  "authors": [
    {
      "name": "Vasily Belosloodcev",
      "email": "vasily.belosloodcev@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "container-interop/container-interop": "^1.2",
    "laminas/laminas-stdlib": "^3.1",
    "laminas/laminas-servicemanager": "^3.1"
  },
  "require-dev": {
    "laminas/laminas-modulemanager": "^2.7.2",
    "laminas/laminas-test": "^3.0.1",
    "friendsofphp/php-cs-fixer": "^2.14.0",
    "phpunit/phpunit": "^7.5.2",
    "php-coveralls/php-coveralls": "^2.1.0",
    "psr/http-message": "^1.0.1",
    "psr/http-server-middleware": "^1.0.0"
  },
  "suggest": {
    "laminas/laminas-modulemanager": "For Laminas",
    "psr/http-message": "For Mezzio",
    "psr/http-server-middleware": "For Mezzio"
  },
  "autoload": {
    "psr-4": {
      "Bupy7\\Zf\\TimeZone\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bupy7\\Zf\\TimeZone\\Test\\": "test/"
    }
  },
  "scripts": {
    "test:run": "php vendor/bin/phpunit --debug",
    "cs:fix": "php vendor/bin/php-cs-fixer fix --config=phpcs.php"
  },
  "config": {
    "sort-packages": true
  }
}
