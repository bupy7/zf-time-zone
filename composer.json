{
  "name": "bupy7/zf-time-zone",
  "description": "Time zone module for Zend Framework 3.",
  "version": "1.0.0",
  "license": "BSD-3-Clause",
  "keywords": [
    "zend",
    "time",
    "zf",
    "zf3",
    "zone"
  ],
  "authors": [
    {
      "name": "Vasily Belosloodcev",
      "email": "vasily.belosloodcev@gmail.com",
      "homepage": "http://mihaly4.ru"
    }
  ],
  "require": {
    "php": "^7.1",
    "container-interop/container-interop": "^1.2",
    "zendframework/zend-stdlib": "^3.1",
    "zendframework/zend-modulemanager": "^2.7.2",
    "zendframework/zend-servicemanager": "^3.1"
  },
  "require-dev": {
    "zendframework/zend-test": "^3.0.1",
    "friendsofphp/php-cs-fixer": "~2.2.3",
    "phpunit/phpunit": ">=5.5 <6.0.0",
    "satooshi/php-coveralls": "^1.0.1"
  },
  "autoload": {
    "psr-4": {
      "Bupy7\\Zf\\TimeZone\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bupy7\\Zf\\TimeZone\\Test\\": "tests/"
    }
  },
  "scripts": {
    "test": "php vendor/bin/phpunit --debug",
    "fixer": "php vendor/bin/php-cs-fixer fix --config=phpcs.php"
  }
}
